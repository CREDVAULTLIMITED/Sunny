{
  "name": "@sunny/utils",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./date": {
      "types": "./dist/date/index.d.ts",
      "import": "./dist/date/index.mjs",
      "module": "./dist/date/index.mjs",
      "require": "./dist/date/index.js"
    },
    "./currency": {
      "types": "./dist/currency/index.d.ts",
      "import": "./dist/currency/index.mjs",
      "module": "./dist/currency/index.mjs",
      "require": "./dist/currency/index.js"
    },
    "./validation": {
      "types": "./dist/validation/index.d.ts",
      "import": "./dist/validation/index.mjs",
      "module": "./dist/validation/index.mjs",
      "require": "./dist/validation/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@sunny/eslint-config": "workspace:*",
    "@sunny/tsconfig": "workspace:*",
    "@types/jest": "^29.5.0",
    "eslint": "^9",
    "jest": "^29.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5"
  }
}
