{
  "name": "@sunny/api-gateway",
  "version": "1.0.0",
  "description": "Sunny Payment Gateway - API Gateway Service",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
