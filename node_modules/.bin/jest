#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/projects/Cred Vault/Sunny/node_modules/.pnpm/jest@27.5.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.10_typescript@5.9.2__utf-8-validate@5.0.10/node_modules/jest/bin/node_modules:/mnt/c/projects/Cred Vault/Sunny/node_modules/.pnpm/jest@27.5.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.10_typescript@5.9.2__utf-8-validate@5.0.10/node_modules/jest/node_modules:/mnt/c/projects/Cred Vault/Sunny/node_modules/.pnpm/jest@27.5.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.10_typescript@5.9.2__utf-8-validate@5.0.10/node_modules:/mnt/c/projects/Cred Vault/Sunny/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/projects/Cred Vault/Sunny/node_modules/.pnpm/jest@27.5.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.10_typescript@5.9.2__utf-8-validate@5.0.10/node_modules/jest/bin/node_modules:/mnt/c/projects/Cred Vault/Sunny/node_modules/.pnpm/jest@27.5.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.10_typescript@5.9.2__utf-8-validate@5.0.10/node_modules/jest/node_modules:/mnt/c/projects/Cred Vault/Sunny/node_modules/.pnpm/jest@27.5.1_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.10_typescript@5.9.2__utf-8-validate@5.0.10/node_modules:/mnt/c/projects/Cred Vault/Sunny/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
